  Support =	 {SNF grant 2100-67712 and RFBF grants N04-01-00427
                  and N02-01-22001},
  Title =	 {On Semimeasures Predicting {Martin-L{\"o}f} Random
                  Sequences},
  Url =		 {http://arxiv.org/abs/0708.2319},
  Volume =	 382,
